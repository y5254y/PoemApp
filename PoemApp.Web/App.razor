<Router AppAssembly="typeof(App).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="routeData" DefaultLayout="typeof(PoemApp.Web.Shared.MainLayout)" />
    </Found>
    <NotFound>
        <LayoutView Layout="typeof(PoemApp.Web.Shared.MainLayout)">
            <p>Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Router>
